<?php

namespace Bruvinsky\HighschoolBundle\Entity;

/**
 * DepartmentSpecialty
 */
class DepartmentSpecialty
{
    /**
     * @var string
     */
    private $id;

    /**
     * @var \Bruvinsky\HighschoolBundle\Entity\Department
     */
    private $idDepartmentDS;

    /**
     * @var \Bruvinsky\HighschoolBundle\Entity\Specialty
     */
    private $idSpecialtyDS;


    /**
     * Get id
     *
     * @return string
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set idDepartmentDS
     *
     * @param \Bruvinsky\HighschoolBundle\Entity\Department $idDepartmentDS
     *
     * @return DepartmentSpecialty
     */
    public function setIdDepartmentDS(\Bruvinsky\HighschoolBundle\Entity\Department $idDepartmentDS = null)
    {
        $this->idDepartmentDS = $idDepartmentDS;

        return $this;
    }

    /**
     * Get idDepartmentDS
     *
     * @return \Bruvinsky\HighschoolBundle\Entity\Department
     */
    public function getIdDepartmentDS()
    {
        return $this->idDepartmentDS;
    }

    /**
     * Set idSpecialtyDS
     *
     * @param \Bruvinsky\HighschoolBundle\Entity\Specialty $idSpecialtyDS
     *
     * @return DepartmentSpecialty
     */
    public function setIdSpecialtyDS(\Bruvinsky\HighschoolBundle\Entity\Specialty $idSpecialtyDS = null)
    {
        $this->idSpecialtyDS = $idSpecialtyDS;

        return $this;
    }

    /**
     * Get idSpecialtyDS
     *
     * @return \Bruvinsky\HighschoolBundle\Entity\Specialty
     */
    public function getIdSpecialtyDS()
    {
        return $this->idSpecialtyDS;
    }
}
